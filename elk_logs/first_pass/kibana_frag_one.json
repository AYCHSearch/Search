curl -o queries_n_clicks.txt -XPOST 'http://elasticsearch2.eanadev.org:9200/logstash-*/_search?pretty' -d '
{
  "size": 500,
  "sort": [
    {
      "client-ipv4": {
        "order": "desc",
        "unmapped_type": "boolean"
      }
    },
    {
      "@timestamp": {
        "order": "desc",
        "unmapped_type": "boolean"
      }
    }
  ],
  "query": {
    "filtered": {
      "query": {
        "query_string": {
                  "query": "url_query:* -url_path:\"/v2/search.json\" -url_query:l%5B* -url_query:wskey=* -url_query:*uri=*",
                  "analyze_wildcard": true
                }
      },
      "filter": {
        "bool": {
          "must": [
            {
              "range": {
                "@timestamp": {
                  "gte": 1470747357713,
                  "lte": 1478526957713,
                  "format": "epoch_millis"
                }
              }
            }
          ],
          "must_not": []
        }
      }
    }
  },
  "fields": [
    "_source"
  ],
  "script_fields": {},
  "fielddata_fields": [
    "@timestamp"
  ]
}'
